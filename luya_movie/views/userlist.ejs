<%- include('header')%>

<table class="ui selectable celled table">
  <thead>
    <tr>
      <th>名字</th>
      <th>时间</th>      
      <th>查看</th>
      <th>更新</th>
      <th>删除</th>
    </tr>
  </thead>
  <tbody>
   <% users.forEach(function(item){ %>
    <tr class="item-id-<%= item._id %>">
      <td><%= item.name %></td>
      <td><%= moment(item.meta.updateAt).format('YYYY-MM-DD') %></td>      
      <td><a href="../movie/<%= item._id %>" target="_blank">查看</a></td>
      <td><a href="../admin/update/<%= item._id %>" target="_blank">修改</a></td>
      <td><button id="userDel" class="ui red button" data-id="<%= item._id %>">删除</button></td>
    </tr>
   <% }) %>
    
  </tbody>
</table>
<%- include('footer')%>